{
    "name": "butterfly-team/datasource",
    "description": "Butterfly application datasource.",
    "keywords": ["butterfly"],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5"
    },
    "autoload": {
        "psr-4": {
            "Butterfly\\DataSource\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Butterfly\\DataSource\\Tests\\": "tests/"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
